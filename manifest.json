{
  "manifest_version": 3,
  "name": "Duplicate Tabs Closer",
  "version": "1.1",
  "description": "Closes duplicate tabs as they open. Includes an on/off toggle.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
"icons": {
  "16": "icon1.png",
  "48": "icon1.png",
  "128": "icon1.png"
},
"action": {
  "default_icon": {
    "16": "icon1.png",
    "48": "icon1.png",
    "128": "icon1.png"
  },
  "default_popup": "popup.html"
}
}